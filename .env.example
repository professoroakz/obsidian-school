# Example environment variables
# Copy this file to .env and fill in your values

# Repository settings
REPO_NAME=obsidian-school
REPO_OWNER=professoroakz

# Sync settings (if using automated sync)
SYNC_INTERVAL=3600  # seconds
AUTO_COMMIT=true

# Backup settings
BACKUP_DIR=./backups
BACKUP_RETENTION_DAYS=30

# Optional: Git settings
GIT_USER_NAME="Your Name"
GIT_USER_EMAIL="your.email@example.com"

# Optional: Publishing settings
PUBLISH_BRANCH=main
PUBLISH_DIR=./

# Optional: Obsidian settings
OBSIDIAN_VAULT_PATH=./
